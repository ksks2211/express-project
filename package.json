{
  "name": "ts-express-project",
  "version": "1.0.0",
  "scripts": {
    "start": "nodemon",
    "build": "./node_modules/.bin/ts-node build.ts",
    "serve": "node ./dist --env=production"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node -r ./src"
  },
  "dependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.3",
    "fs-extra": "^10.0.1",
    "http-errors": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
